{
    "name": "sanjabteam/ticket",
    "description": "Ticket package for sanjab admin panel.",
    "keywords": [
        "sanjabteam",
        "ticket"
    ],
    "homepage": "https://github.com/sanjabteam/ticket",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "amir alizadeh",
            "email": "amiralizadeh9480@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7|^8",
        "sanjabteam/sanjab": "*"
    },
    "autoload": {
        "psr-4": {
            "SanjabTicket\\": "src",
            "SanjabTicket\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SanjabTicket\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SanjabTicket\\SanjabTicketServiceProvider"
            ],
            "aliases": {
                "SanjabTicket": "SanjabTicket\\SanjabTicketFacade"
            }
        }
    }
}
